{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FF008C",
          "style": "plain",
          "template": "<#602EBD>\u250f[</>{{ .UserName }}<#602EBD>]</>",
          "type": "session"
        },
        {
          "foreground": "#FF008C",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#602EBD>--[</>{{ .HEAD }}<#12BDDE>{{ .BranchStatus }}</>{{ if .Working.Changed }}<#12BDDE> \u25cf </>{{ end }}{{ if .Staging.Changed }}<#12BDDE> \u25cf </>{{ end }}<#602EBD>]</>",
          "type": "git"
        },
        {
          "foreground": "#F5A623",
          "style": "plain",
          "template": "<#602EBD>--[</>{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}<#602EBD>]</>",
          "type": "python"
        },
        {
          "foreground": "#27F523",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " \ue71e",
            "yarn_icon": "ðŸ§¶"
          },
          "style": "plain",
          "template": "<#602EBD>--[</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#602EBD>]</>",
          "type": "node"
        },
        {
          "foreground": "#FF008C",
          "style": "plain",
          "template": "<#602EBD>--[</>{{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#602EBD>]</>",
          "type": "aws"
        },
        {
          "foreground": "#FF008C",
          "style": "plain",
          "template": "<#602EBD>--[</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#602EBD>]</>",
          "type": "kubectl"
        },
        {
          "foreground": "#d8dee9",
          "style": "plain",
          "template": "<#602EBD>[</>\uf0e7<#602EBD>]</>",
          "type": "root"
        },
        {
          "foreground": "#d8dee9",
          "style": "plain",
          "template": "<#602EBD>[x</>{{ .Meaning }}<#602EBD>]</>",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#FF008C",
          "style": "plain",
          "template": "<#602EBD>[</>{{ .FormattedMs }}<#602EBD>]</>",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#C3C3C3",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "<#602EBD>\u2516[</>{{ .Path }}<#602EBD>]</>",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#C3C3C3",
          "style": "plain",
          "template": " \ue602 ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
